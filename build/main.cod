//!cofoh.com/css-on-diet-1.7
@cod-include {
    reset.cod
}
@cod-defines {
    offwhite #fafafa
}
@font-face {
    fof 'alegreya_sansregular';
    src url('/theme/fonts/alegreyasans-regular-webfont.eot')
    src url('/theme/fonts/alegreyasans-regular-webfont.eot?#iefix') format('embedded-opentype'), \
    url('/theme/fonts/alegreyasans-regular-webfont.woff') format('woff'), \
    url('/theme/fonts/alegreyasans-regular-webfont.ttf') format('truetype'), \
    url('/theme/fonts/alegreyasans-regular-webfont.svg#alegreya_sansregular') format('svg')
    fow nm
    fot nm
}
body {
    miw %%
    bac offwhite
    fof 'alegreya_sansregular'
}
#headerblock:after {
    cle both
    vis hi
    dis bl
    hei 0
    con "."
}
header#sitename {
    flo left
}
header#sitename h1 {
    dis ib
    fos 3.5e
    tes 1p 1p 2p black
}
header#sitename h1 a {
    ted no
    col dodgerblue
}
header#sitename h1 a:hover {

}
header#sitename h2 {
    dis ib
    fos 1.7e
    les .05rem
    col darkgray
}
form#searchbox {
    dis ib
}
nav#menu {
    cle both
    mab 1e
}
nav ul {
    li- no outside no
}
nav li {
    dis ib
}
